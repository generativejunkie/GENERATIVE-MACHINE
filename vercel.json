{
  "cleanUrls": true,
  "redirects": [
    {
      "source": "/info",
      "destination": "/info.html",
      "permanent": true
    },
    {
      "source": "/monitor",
      "destination": "/monitor.html",
      "permanent": true
    },
    {
      "source": "/talk",
      "destination": "/talk.html",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "generativejunkie.net"
        }
      ],
      "destination": "https://www.generativejunkie.net/:path*",
      "permanent": true
    }
  ]
}