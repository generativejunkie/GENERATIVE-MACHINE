{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/info",
      "destination": "/info/fa2025",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "generativejunkie.net"
        }
      ],
      "destination": "https://www.generativejunkie.net/:path*",
      "permanent": true
    }
  ]
}