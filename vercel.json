{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/singularity",
      "destination": "/singularity.html"
    },
    {
      "source": "/info/:slug",
      "destination": "/info/:slug.html"
    }
  ],
  "redirects": [
    {
      "source": "/info",
      "destination": "/info/fa2025",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "generativejunkie.net"
        }
      ],
      "destination": "https://www.generativejunkie.net/:path*",
      "permanent": true
    }
  ]
}